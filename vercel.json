{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/:path((?!api/|\\.well-known/).*)",
      "has": [
        {
          "type": "host",
          "value": "yoombaa.com"
        }
      ],
      "destination": "https://www.yoombaa.com/:path",
      "permanent": true
    }
  ]
}

